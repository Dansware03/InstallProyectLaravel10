{
  "name": "dansware03/laravel-installer",
  "description": "Asistente de instalaci√≥n interactivo para aplicaciones Laravel 10",
  "type": "library",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "installer",
    "setup",
    "wizard"
  ],
  "support": {
    "email": "dansware2003@gmail.com",
    "issues": "https://github.com/Dansware03/laravel-installer/issues",
    "source": "https://github.com/Dansware03/laravel-installer"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Maiker Bravo",
      "email": "dansware2003@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0",
    "illuminate/console": "^10.0",
    "illuminate/http": "^10.0",
    "illuminate/routing": "^10.0",
    "illuminate/view": "^10.0",
    "illuminate/database": "^10.0",
    "symfony/process": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Dansware03\\LaravelInstaller\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Dansware03\\LaravelInstaller\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dansware03\\LaravelInstaller\\LaravelInstallerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}