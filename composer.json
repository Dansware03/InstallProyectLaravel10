{
    "name": "tuusuario/laravel-installer",
    "description": "Asistente de instalaci√≥n interactivo para aplicaciones Laravel 10",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tu Nombre",
            "email": "tu@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/view": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "TuUsuario\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TuUsuario\\LaravelInstaller\\InstallationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}