{
  "name": "dansware03/laravel-installer",
  "description": "Asistente de instalaci√≥n interactivo para aplicaciones Laravel 10",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Maiker Bravo",
      "email": "Dansware2003@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0",
    "illuminate/console": "^10.0",
    "illuminate/http": "^10.0",
    "illuminate/routing": "^10.0",
    "illuminate/view": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Dansware\\LaravelInstaller\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dansware\\LaravelInstaller\\InstallationServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "https://github.com/Dansware03/laravel-installer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
